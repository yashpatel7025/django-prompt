{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "strict": true,
    "lib": ["es2016", "es2018", "dom"],
    "target": "es2018",
    "sourceMap": true,
    "module": "esnext",
